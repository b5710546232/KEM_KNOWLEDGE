{
  "name": "nature-drink",
  "version": "0.0.1",
  "description": "Online water and bottle store",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack-dev-server --hot --inline --progress --open",
    "start-server": "NODE_ENV=production webpack-dev-server --hot --inline --progress",
    "start-win": "cross-env NODE_ENV=production webpack-dev-server --hot --inline --progress --open",
    "build": "cross-env NODE_ENV=production npm run test && webpack --progress --colors",
    "build-server": "NODE_ENV=production  webpack --display-chunks --optimize-min-chunk-size 10000 --optimize-max-chunks 10 --display-reasons --progress --color",
    "test": "mocha -r mock-local-storage --compilers js:babel-register --require ./test/test-helper --recursive ",
    "test:watch": "npm run test -- --watch --watch-extensions jsx"
  },
  "author": "starburststream.company",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.6.12",
    "classnames": "^2.2.5",
    "immutable": "^3.8.1",
